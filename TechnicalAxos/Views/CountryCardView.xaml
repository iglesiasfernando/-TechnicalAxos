<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TechnicalAxos.Views.CountryCardView">
    <ContentView.Content>
        <StackLayout Margin="10,0,0,10" Orientation="Vertical">
            <Label Text="{Binding Name.Official}" FontAttributes="Bold"/>
            <Label Text="{Binding Name.Common}" />
        </StackLayout>
    </ContentView.Content>
</ContentView>